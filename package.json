{
  "name": "AwesomeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "yarn run watchman-reset && react-native start --reset-cache",
    "start-debug": "yarn run watchman-reset && react-native start --reset-cache --experimental-debugger",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 15 Pro Max'",
    "lint": "eslint .",
    "test": "jest  --transformIgnorePatterns 'node_modules/(?!my-library-dir)/'",
    "generate": "plop && prettier src/components/index.ts --write && prettier src/utils/index.ts --write",
    "install-npm-packages": "yarn install",
    "watchman-reset": "watchman shutdown-server",
    "link-assets": "npx react-native-asset",
    "type-check": "tsc --noEmit",
    "macos:open:ios": "open ios/AwesomeProject.xcworkspace",
    "macos:open:android": "open -a 'Android Studio' android",
    "pod-install": "cd ios && pod install && cd ..",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@georstat/react-native-image-cache": "^3.1.0",
    "@gorhom/bottom-sheet": "^4.6.3",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^2.0.1",
    "@sentry/react-native": "^5.23.1",
    "@shopify/flash-list": "^1.6.4",
    "axios": "^1.6.4",
    "date-fns": "^3.6.0",
    "i18next": "^23.7.15",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-i18next": "^14.0.0",
    "react-native": "0.73.8",
    "react-native-animatable": "^1.4.0",
    "react-native-device-info": "^11.1.0",
    "react-native-file-access": "^3.1.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "^3.2.0",
    "react-native-reanimated": "^3.12.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^14.1.0",
    "react-redux": "^9.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.5",
    "@types/react": "^18.2.6",
    "@types/react-native-dotenv": "^0.2.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.13",
    "@types/redux-persist": "^4.3.1",
    "@welldone-software/why-did-you-render": "^8.0.1",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "patch-package": "^8.0.0",
    "plop": "^4.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.9",
    "react-test-renderer": "18.2.0",
    "redux-saga-test-plan": "^4.0.6",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
